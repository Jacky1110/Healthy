<resources>

    <style name="Widget.Theme.Healthy.MyView" parent="">
        <item name="android:background">@color/gray_400</item>
        <item name="ewlLabelColor">@color/txtcolor</item>
    </style>

    <style name="Fragment_Account" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingStart">@dimen/fragment_horizontal_padding</item>
        <item name="android:paddingEnd">@dimen/fragment_horizontal_padding</item>
        <item name="android:paddingTop">@dimen/fragment_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/fragment_vertical_padding</item>
        <item name="android:background">@drawable/background_gradient_main</item>
    </style>

    <style name="Fragment_Title_TextView" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginTop">@dimen/fragment_account_title_top_margin</item>
        <item name="android:textSize">@dimen/fragment_account_title_textsize</item>
    </style>

    <style name="EditWithLabelView_Label" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/editwithlabel_top_margin</item>
        <item name="android:layout_marginBottom">@dimen/editwithlabel_internal_offset_vertical</item>
        <item name="android:textColor">@color/txtcolor</item>
        <item name="android:textSize">@dimen/editwithlabel_label_textsize</item>
    </style>

    <style name="EditWithLabelView_Data" parent="Widget.AppCompat.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_marginStart">@dimen/editwithlabel_data_margin_start</item>
        <item name="android:paddingTop">@dimen/editwithlabel_data_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/editwithlabel_data_padding_vertical</item>
        <item name="android:textSize">@dimen/editwithlabel_data_textsize</item>
        <item name="android:textColor">@color/gray_20</item>
        <item name="android:textColorHint">@color/gray_60</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="AccountMainListItemTextView" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/account_listitem_textsize</item>
        <item name="android:layout_marginTop">@dimen/account_listitem_vertical_margin</item>
        <item name="android:layout_marginStart">@dimen/account_listitem_start_margin</item>
    </style>

    <style name="AccountMainListItemImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/account_listitem_vertical_margin</item>
        <item name="android:layout_marginBottom">@dimen/account_listitem_vertical_margin</item>
        <item name="android:layout_marginEnd">@dimen/account_listitem_end_margin</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_gravity">end</item>
    </style>

    <style name="toolBarIcon">
        <item name="android:layout_height">@dimen/toolbar_height</item>
        <item name="android:layout_width">44dp</item>
        <item name="android:padding">8dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
    </style>

    <style name="ivHomeTabIcon">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_margin">7dp</item>
    </style>
    <style name="ivMemberArrowIcon">
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:src">@drawable/ic_right_arrow</item>
        <item name="android:layout_marginEnd">12dp</item>
    </style>
    <style name="vMemberLine0">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/c_eeeeee_100</item>
    </style>
    <style name="tvMemberList">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/c_4b525b_100</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>
    <style name="tvMemberTabIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/c_4b525b_100</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
    </style>
    <style name="ivMemberTabIcon">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
    </style>
    <style name="siv_circle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="tvMemberText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/c_b21b25_100</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>
    <style name="DatePickerTheme" parent="android:Theme.Holo.Light.Dialog.MinWidth">
        <item name="android:windowBackground">@android:color/transparent</item>

    </style>
</resources>